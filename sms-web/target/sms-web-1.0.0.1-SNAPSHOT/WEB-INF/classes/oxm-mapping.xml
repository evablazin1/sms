<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.exolab.org/mapping.dtd">
<mapping>
    <class name="com.education.restful.request.model.Request">
        <field name="name" type="string">
            <bind-xml name="name" node="element"/>
        </field>
        <field name="value" type="string">
            <bind-xml name="value" node="element"/>
        </field>
	</class>
	<class name="com.education.restful.response.model.Response">
        <field name="status" type="string">
            <bind-xml name="status" node="element"/>
        </field>
        <field name="message" type="string">
            <bind-xml name="message" node="element"/>
        </field>        
    </class>
</mapping>